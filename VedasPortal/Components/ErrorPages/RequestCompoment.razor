
<div class="col-12 col-md-6 col-sm-12">
    <div class="card">
        <div class="card-header">
            <h4>@RequestHeader</h4>
        </div>
        <div class="card-body">
            <div class="empty-state" data-height="400">
                @EmptyState
            </div>
        </div>
    </div>
</div>

@code {


    [Parameter]
    public RenderFragment EmptyState { get; set; }

    [Parameter]
    public RenderFragment RequestHeader { get; set; }



}