
<div class="@("alert alert-"+alertStyle.ToString())">
	<div class="alert-title">
		@AlertTitleName
	</div>
	@Description
</div>


@code {
	[Parameter]
	public Style alertStyle { get; set; }
	[Parameter]
	public RenderFragment Description { get; set; }

	[Parameter]
	public RenderFragment AlertTitleName { get; set; }

	
}
